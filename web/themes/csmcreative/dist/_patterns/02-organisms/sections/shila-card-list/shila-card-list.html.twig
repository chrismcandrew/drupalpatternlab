<div class="card-list">
  {% block cards %}
    {% for card in cards %}
      {% include '@molecules/components/shila-card/shila-card.html.twig'
        with {
          'additional_classes': card.additional_classes,
          'image': card.image,
          'title': card.title,
          'content': card.content,
          'link_url': card.link_url,
          'link_text': card.link_text
        }
      %}
    {% endfor %}
  {% endblock %}
</div>
